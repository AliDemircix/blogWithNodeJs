<ul class="pagination col-lg-12 mx-auto">
  <li class="page-item mr-auto <%=newPage==1?'disabled': '' %>">
    <a class="page-link" href="?page=<%=parseInt(newPage)-1%>">Previous</a>
  </li>
  <% for( let index = 1; index <= totalPages; index++ ) { %>
  <li class="page-item <%=newPage==index ? 'active': '' %>">
    <a class="page-link" href="?page=<%=index%> "><%-index %> </a>
  </li>
  <% } %>

  <li class="page-item ml-auto <%=newPage==totalPages?'disabled': '' %>">
    <a class="page-link" href="?page=<%=parseInt(newPage)+1%>">Next</a>
  </li>
</ul>
